<UserControl x:Class="theHunterLog.Controls.LoadOutWeaponLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:theHunterLog.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal">
        <ComboBox x:Name="cbWeapon" Width="125" SelectionChanged="cbWeapon_SelectionChanged"></ComboBox>
        <ComboBox x:Name="cbAmmunition" Width="125"></ComboBox>
        <Button x:Name="btnDelLine" Width="50" Click="btnDelLine_Click" >-</Button>
    </StackPanel>
</UserControl>
